<com.android.inputmethod.skeyboard.LatinKeyboardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.skeyboard"
        android:id="@+id/LatinkeyboardBaseView"
        android:layout_alignParentBottom="true"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/sip_bg_keyboard"

        latin:keyBackground="@drawable/btn_keyboard_key_white"
		latin:keyHintPopup="@drawable/hint_popup_white"
        latin:keyTextColor="@color/latinkeyboard_key_color_black"
        latin:shadowColor="@color/latinkeyboard_key_color_white"
        latin:keyIconsSet = "white"
        latin:keyPreviewBackground="@drawable/keyboard_key_feedback_sip"
        latin:keyPreviewSlideBackground="@drawable/keyboard_key_feedback_sip"
        />
